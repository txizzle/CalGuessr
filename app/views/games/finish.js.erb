<% if @game.name == nil and (@highscores.length < 10 or @highscores[-1] > @game.score) %>
  $("#game-modal").html("<%= escape_javascript(render 'inputname') %>")
  $("#game-modal").modal("show")
<% else %>
  $("#game-modal").html("<%= escape_javascript(render 'finish') %>")
  $("#game-modal").modal("show")
<% end %>
